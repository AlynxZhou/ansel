[Desktop Entry]
Name=Ansel
_GenericName=Virtual Lighttable and Darkroom
_Comment=Organize and develop images from digital cameras

X-GNOME-FullName=Ansel

Version=1.0

Type=Application
Categories=Graphics;Photography;GTK;
# TRANSLATORS: Search terms to find this application. Do NOT translate or localize the semicolons! The list MUST also end with a semicolon!
_Keywords=graphics;photography;raw;

Exec=${DESKTOP_ANSEL_BINARY} %U
TryExec=${DESKTOP_ANSEL_BINARY}
Terminal=false
StartupNotify=true

MimeType=application/x-ansel;image/x-dcraw;image/jpeg;image/jpg;image/jp2;image/png;image/tiff;image/x-portable-pixmap;image/x-portable-floatmap;image/x-exr;

Icon=${DESKTOP_ANSEL_ICON}
StartupWMClass=ansel
